{
    "$schema":  "./node_modules/@angular/cli/lib/config/schema.json",
    "version":  1,
    "newProjectRoot":  "projects",
    "projects":  {
                     "movie-booking-frontend":  {
                                                    "projectType":  "application",
                                                    "schematics":  {
                                                                       "@schematics/angular:component":  {
                                                                                                             "style":  "scss",
                                                                                                             "standalone":  false
                                                                                                         },
                                                                       "@schematics/angular:directive":  {
                                                                                                             "standalone":  false
                                                                                                         },
                                                                       "@schematics/angular:pipe":  {
                                                                                                        "standalone":  false
                                                                                                    }
                                                                   },
                                                    "root":  "",
                                                    "sourceRoot":  "src",
                                                    "prefix":  "app",
                                                    "architect":  {
                                                                      "build":  {
                                                                                    "builder":  "@angular-devkit/build-angular:application",
                                                                                    "options":  {
                                                                                                    "outputPath":  "dist/movie-booking-frontend",
                                                                                                    "index":  "src/index.html",
                                                                                                    "browser":  "src/main.ts",
                                                                                                    "polyfills":  [
                                                                                                                      "zone.js"
                                                                                                                  ],
                                                                                                    "tsConfig":  "tsconfig.app.json",
                                                                                                    "inlineStyleLanguage":  "scss",
                                                                                                    "assets":  [
                                                                                                                   {
                                                                                                                       "glob":  "**/*",
                                                                                                                       "input":  "public"
                                                                                                                   }
                                                                                                               ],
                                                                                                    "styles":  [
                                                                                                                   "src/styles.scss"
                                                                                                               ],
                                                                                                    "scripts":  [

                                                                                                                ],
                                                                                                    "allowedCommonJsDependencies":  [
                                                                                                                                        "dayjs",
                                                                                                                                        "dayjs/plugin/utc",
                                                                                                                                        "dayjs/plugin/timezone"
                                                                                                                                    ]
                                                                                                },
                                                                                    "configurations":  {
                                                                                                           "production":  {
                                                                                                                              "budgets":  [
                                                                                                                                              {
                                                                                                                                                  "type":  "initial",
                                                                                                                                                  "maximumWarning":  "2MB",
                                                                                                                                                  "maximumError":  "3MB"
                                                                                                                                              },
                                                                                                                                              {
                                                                                                                                                  "type":  "anyComponentStyle",
                                                                                                                                                  "maximumWarning":  "4kB",
                                                                                                                                                  "maximumError":  "8kB"
                                                                                                                                              }
                                                                                                                                          ],
                                                                                                                              "outputHashing":  "all",
                                                                                                                              "fileReplacements":  [

                                                                                                                                                   ]
                                                                                                                          },
                                                                                                           "development":  {
                                                                                                                               "optimization":  false,
                                                                                                                               "extractLicenses":  false,
                                                                                                                               "sourceMap":  true,
                                                                                                                               "fileReplacements":  [
                                                                                                                                                        {
                                                                                                                                                            "with":  "src/environments/environment.development.ts",
                                                                                                                                                            "replace":  "src/environments/environment.ts"
                                                                                                                                                        }
                                                                                                                                                    ]
                                                                                                                           }
                                                                                                       },
                                                                                    "defaultConfiguration":  "production"
                                                                                },
                                                                      "serve":  {
                                                                                    "builder":  "@angular-devkit/build-angular:dev-server",
                                                                                    "configurations":  {
                                                                                                           "production":  {
                                                                                                                              "buildTarget":  "movie-booking-frontend:build:production"
                                                                                                                          },
                                                                                                           "development":  {
                                                                                                                               "buildTarget":  "movie-booking-frontend:build:development"
                                                                                                                           }
                                                                                                       },
                                                                                    "defaultConfiguration":  "development"
                                                                                },
                                                                      "extract-i18n":  {
                                                                                           "builder":  "@angular-devkit/build-angular:extract-i18n"
                                                                                       },
                                                                      "test":  {
                                                                                   "builder":  "@angular-devkit/build-angular:karma",
                                                                                   "options":  {
                                                                                                   "polyfills":  [
                                                                                                                     "zone.js",
                                                                                                                     "zone.js/testing"
                                                                                                                 ],
                                                                                                   "tsConfig":  "tsconfig.spec.json",
                                                                                                   "inlineStyleLanguage":  "scss",
                                                                                                   "assets":  [
                                                                                                                  {
                                                                                                                      "glob":  "**/*",
                                                                                                                      "input":  "public"
                                                                                                                  }
                                                                                                              ],
                                                                                                   "styles":  [
                                                                                                                  "src/styles.scss"
                                                                                                              ],
                                                                                                   "scripts":  [

                                                                                                               ]
                                                                                               }
                                                                               }
                                                                  }
                                                }
                },
                "cli": {
                  "analytics": "7a893d29-ba47-41e0-afb2-be575c9c70af"
                }
}
